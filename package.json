{
	"publisher": "AppLandInc",
	"name": "appland",
	"displayName": "AppLand",
	"description": "AppLand code analysis and visualization",
	"version": "0.1.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/applandinc/vscode-appland"
	},
	"engines": {
		"vscode": "^1.51.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"license": "MIT",
	"main": "./out/extension.js",
	"contributes": {
		"customEditors": [
			{
				"viewType": "appland.appMapFile",
				"displayName": "AppMap file",
				"selector": [
					{
						"filenamePattern": "*.appmap.json"
					}
				],
				"priority": "default"
			}
		],
		"commands": [
			{
				"command": "appland.openMostRecentlyModifiedAppMap",
				"title": "AppLand: Open most recently modified AppMap file"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"lint": "eslint . --ext .ts",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js && node ./tape/**-test.js",
		"compile": "webpack --mode=production",
		"watch": "webpack --watch --mode=development",
		"package": "vsce package"
	},
	"devDependencies": {
		"@babel/core": "^7.12.10",
		"@babel/preset-env": "^7.12.11",
		"@babel/preset-typescript": "^7.12.7",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.0",
		"@types/node": "^12.19.12",
		"@types/vscode": "^1.52.0",
		"@typescript-eslint/eslint-plugin": "^4.12.0",
		"@typescript-eslint/parser": "^4.12.0",
		"babel-loader": "^8.2.2",
		"css-loader": "^5.0.1",
		"eslint": "^7.17.0",
		"eslint-plugin-import": "^2.22.1",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"sass": "^1.32.2",
		"sass-loader": "^10.1.0",
		"style-loader": "^2.0.0",
		"ts-loader": "^8.0.14",
		"tslib": "^2.1.0",
		"typescript": "^4.1.3",
		"vsce": "^1.83.0",
		"vscode-test": "^1.4.0",
		"webpack": "^5.12.1",
		"webpack-cli": "^4.3.1"
	},
	"dependencies": {
		"@appland/diagrams": "github:applandinc/diagrams-js#master",
		"@appland/models": "github:applandinc/models-js#master",
		"bootstrap": "^4.5.3",
		"bootstrap-autocomplete": "^2.3.7",
		"d3": "^5.16.0",
		"jquery": "^3.5.1",
		"popper.js": "^1.16.1"
	}
}
